
<div class="game mt-5 mb-3 mx-auto borderS">
    <div class="text-center">
        <img src="~/img/gamesposters/2077.png" />
    </div>

    <div class="text-center">
        <h4><a asp-area="" asp-controller="Home" asp-action="GamePage" asp-route-gameId="">Cyberpunk 2077</a></h4>
    </div>

    <div class="m-1">
        <p>
            Описание ааааааааааааааааааааааааааааааа
        </p>
    </div>

    <div class="d-flex flex-wrap ">
        <div class="bg-ganre m-1 p-1 text-white">Жанры</div>
        <div class="bg-ganre m-1 p-1 text-white">Жанры</div>
        <div class="bg-ganre m-1 p-1 text-white">Жанры</div>
        <div class="bg-ganre m-1 p-1 text-white">Жанры</div>
    </div>

    <div class="d-flex flex-wrap ">
        <div class="bg-glables m-1 p-1 text-white">Lable</div>
        <div class="bg-glables m-1 p-1 text-white">Lable</div>
        <div class="bg-glables m-1 p-1 text-white">Lable</div>
        <div class="bg-glables m-1 p-1 text-white">Lable</div>
    </div>

    <div class="d-flex flex-wrap  h5">
        <div class="m-1 p-1">
            333 BYN
        </div>

        @if (User.Identity.IsAuthenticated)
        {
            <div class="btn-group mx-auto mb-3  float-end">
                <button type="button" class="btn btn-outline-primary" data-bs-toggle="tooltip" data-bs-placement="right" title="Добавить в список желаемого"><i class="fas fa-star"></i></button>
                <button type="button" data-gameId="" class="btn btn-success btn-add-shopcart-js">В Корзину</button>
            </div>
        }

    </div>

</div>

